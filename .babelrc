{
    "plugins": [
      [
        "module-resolver",
        {
          "root": ["."],
          "alias": {
            "styles": "./styles",
            "components": "./shared",
            "utilities": "./utilities"
          },
          "cwd": "babelrc"
        }
      ],
      [
        "wrap-in-js",
        {
          "extensions": ["css$", "scss$"]
        }
      ],
      [
        "transform-decorators-legacy"
      ]
    ],
    "presets": [
      ["next/babel", {
        "preset-env": {
          "targets": {
            "browsers": ["last 2 versions", "chrome >= 41"]
          }
        }
      }]
    ],
    "ignore": []
  }